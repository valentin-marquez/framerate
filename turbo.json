{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build",
        "check-types"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "dist/**",
        ".wrangler/**"
      ]
    },
    "tracker#build": {
      "outputs": [
        "dist/**"
      ]
    },
    "web#build": {
      "outputs": [
        "dist/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": [
        "check-types"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ],
      "outputs": []
    },
    "generate:types": {
      "cache": false
    },
    "db:push": {
      "cache": false,
      "interactive": true
    },
    "group-variants": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "biome:lint": {
      "outputs": []
    }
  }
}
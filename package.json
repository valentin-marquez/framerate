{
  "name": "framerate",
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "turbo": "^2.6.1",
    "typescript": "^5.0.0"
  },
  "packageManager": "^bun@1.2.9",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean && rm -rf node_modules",
    "check-types": "turbo run check-types",
    "format": "biome format --write .",
    "check": "biome check .",
    "db:push": "turbo run db:push",
    "generate:types": "turbo run generate:types",
    "dev:web": "turbo run dev --filter=web",
    "dev:api": "turbo run dev --filter=api",
    "dev:collector": "turbo run dev --filter=collector",
    "dev:tracker": "turbo run dev --filter=tracker"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}